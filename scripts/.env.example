# Cloudflare Worker Configuration
# Copy to .env and fill in your values

# Cloudflare account (from dashboard.cloudflare.com)
CF_ACCOUNT_ID=your_account_id_here

# Cloudflare API token (needs Workers + R2 permissions)
CF_API_TOKEN=your_api_token_here

# Auth token for the Worker /enqueue endpoint
# Generate: python -c "import secrets; print(secrets.token_urlsafe(32))"
WORKER_AUTH_TOKEN=your_auth_token_here

# Worker URL (update after deployment)
WORKER_URL=https://austlii-scraper.YOUR_SUBDOMAIN.workers.dev

# R2 S3-compatible API credentials (from R2 dashboard â†’ Manage R2 API Tokens)
R2_BUCKET_NAME=austlii-case-results
R2_ACCESS_KEY_ID=your_r2_access_key_here
R2_SECRET_ACCESS_KEY=your_r2_secret_key_here
